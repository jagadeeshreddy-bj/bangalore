pipeline{
    agent any
    
    environment{
      PATH = "/opt/maven3/bin:$PATH"
    }
    stages{
        stage("welcome"){
            steps{
                git credentialsId: 'github', url: 'https://github.com/jagadeeshreddy-bj/bangalore.git'
            }
        stage("Maven Build"){
            steps{
            sh "mvn clean package"
            }
        }
            
        }
    }
}
